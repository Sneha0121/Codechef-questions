#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    bool isPrime = true;
	    if(n<2) isPrime = false;
	    for(int i=2; i<n; i++){
	        if(n%i == 0) isPrime= false;
	    }
	    
	    if(isPrime) cout<<"Yes\n";
	    else cout<<"No\n";
	}

}
