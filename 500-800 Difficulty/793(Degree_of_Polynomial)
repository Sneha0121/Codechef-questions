#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int coeff[n];
	    for(int i=0; i<n;i++) cin>>coeff[i];
	    if(n==1) cout<<0<<endl;
	    else{
	        while(coeff[n-1]==0) n--;
	        cout<<n-1<<endl;
	    }
	}

}
