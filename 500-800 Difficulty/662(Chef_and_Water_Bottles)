#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,x,k;
	    cin>>n>>x>>k;
	    int count = 0;

	    for(int i=0;i<n && k>=x;i++){
	        if(k>=x){
	            count++;
	            k -= x;
	        }
	    }
	    cout<<count<<endl;
	}

}
