#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x;
	    cin>>x;
	    int reversed_Number = 0;
	    while(x!=0){
	        int digit = x%10;
	        reversed_Number = reversed_Number*10 +digit;
	        x /=10;
	    }
	    cout<<reversed_Number<<endl;
	}

}
